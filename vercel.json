{
	"version": 2,
	"builds": [
	  {
		"src": "./src/app.js",
		"use": "@vercel/node"
	  },
	  {
		"src": "public/**",
		"use": "@vercel/static"
	  },
	  {
		"src": "./node_modules/.bin/sequelize db:seed:all",
		"use": "@vercel/node"
	  }
	],
	"routes": [
	  {
		"src": "/((?!.*\\.\\w+$).*)",
		"dest": "src/app.js"
	  },
	  {
		"src": "/(.+\\.[a-z]+)$",
		"dest": "/public/$1"
	  },
	  {
		"src": "src/database/seeders",
		"methods": ["POST"],
		"dest": "node_modules/sequelize-cli/lib/sequelize db:seed:all"
	  }
	]
  }